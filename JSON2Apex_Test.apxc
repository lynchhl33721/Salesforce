@IsTest
public class JSON2Apex_Test {
	
	// This test method should give 100% coverage
	static testMethod void testParse() {
		String json = '{'+
		'    \"animal\": {'+
		'        \"id\": 1,'+
		'        \"name\": \"chicken\",'+
		'        \"eats\": \"chicken food\",'+
		'        \"says\": \"cluck cluck\"'+
		'    }'+
		'}';
		JSON2Apex r = JSON2Apex.parse(json);
		System.assert(r != null);

		json = '{\"TestAMissingObject\": { \"TestAMissingArray\": [ { \"TestAMissingProperty\": \"Some Value\" } ] } }';
		JSON2Apex objJSON2Apex = new JSON2Apex(System.JSON.createParser(json));
		System.assert(objJSON2Apex != null);
		System.assert(objJSON2Apex.animal == null);

		json = '{\"TestAMissingObject\": { \"TestAMissingArray\": [ { \"TestAMissingProperty\": \"Some Value\" } ] } }';
		JSON2Apex.Animal objAnimal = new JSON2Apex.Animal(System.JSON.createParser(json));
		System.assert(objAnimal != null);
		System.assert(objAnimal.id == null);
		System.assert(objAnimal.name == null);
		System.assert(objAnimal.eats == null);
		System.assert(objAnimal.says == null);
	}
}